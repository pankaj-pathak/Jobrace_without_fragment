<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <solid
                android:color="#f57c00"/>
            <corners
                android:radius="5dp"/>
        </shape>
    </item>
    <item >
        <shape
            android:shape="rectangle">
            <solid
                android:color="#ff9800"/>
            <corners
                android:radius="5dp"/>
        </shape>
    </item>
</selector>